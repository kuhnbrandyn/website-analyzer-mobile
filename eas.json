{
  "cli": {
    "version": ">= 3.13.0"
  },
  "build": {
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "resourceClass": "default"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "default"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
